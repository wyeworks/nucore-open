- if order_detail.order.cross_core_project.present?
  - if order_detail.order.cross_core_project.facility == current_facility
    %i.fa.fa-building{ data: { toggle: "tooltip" }, title: t(".cross_core_icon_tooltip", facility: order_detail.order.cross_core_project.facility.abbreviation)}
  - else
    %i.fa.fa-users{ data: { toggle: "tooltip" }, title: t(".cross_core_icon_tooltip", facility: order_detail.order.cross_core_project.facility.abbreviation)}
