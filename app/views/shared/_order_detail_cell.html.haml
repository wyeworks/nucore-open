%td.user-order-detail
  .order-detail-description= order_detail_description(od)
  - if od.survey_completed?
    .order-detail-extra= link_to 'View Order Form', od.external_service_receiver.response_data, :target => '_blank'
  - od.stored_files.template_result.each do |stored_file|
    .order-detail-extra= link_to 'View Order File', stored_file.file.url
  - if od.note.present?
    .order-detail-extra.order-detail-note= "Note: #{od.note}"