= content_for :h1 do
  = current_facility
= content_for :sidebar do
  = render partial: "admin/shared/sidenav_admin", locals: { sidenav_tab: "price_groups" }
= content_for :tabnav do
  = render partial: "admin/shared/tabnav_price_group", locals: { secondary_tab: "accounts" }

%h2 Add Price Group Member
%fieldset
  = form_tag url_for(action: "search_results"), id: "ajax_form", method: :get do
    = label_tag :search_term, t(".label.search_term")
    %br
    = text_field_tag :search_term, nil, size: 30, class: "search-query"
    = submit_tag "Search", class: "btn"
%br
#result
