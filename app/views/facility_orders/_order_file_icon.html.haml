- if od.survey_completed?
  .inline-icons
    = link_to od.external_service_receiver.show_url, :target => '_blank' do
      %i.icon-file-text{:data => { :toggle => 'tooltip' }, :title => t('order_details.show.link.view.order_form') }
- od.stored_files.template_result.each do |stored_file|
  .inline-icons
    = link_to facility_template_result_path(stored_file) do
      %i.icon-file-text{:data => { :toggle => 'tooltip' }, :title => t('order_details.show.link.view.order_file') }
